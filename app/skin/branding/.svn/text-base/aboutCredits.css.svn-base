/** 
*******************************************************************************

BROWSER CREDITS PAGE 

Shown when you load "about:credits" in the browser.

*******************************************************************************
*/ 

.container {
	-moz-border-radius-topleft: 8px;
	-moz-border-radius-topright: 8px;
	-moz-border-radius-bottomleft: 8px;
	-moz-border-radius-bottomright: 8px;
	background-color:rgb(235,235,235);
}

.ircnick {
	font-size: 80%;
	font-style: italic;
}

h3.letter, h3.lang {
	padding-bottom: 0;
	margin-bottom: 0;
}

p.developers,
p.champs {
	padding: 0;
	margin: 0;
}

p.champs {
  margin-top: 1em;
}

ul.contributors,
ul.contributors li {
	list-style: none;
	display: inline;
	margin: 0;
	padding: 0;
  
}

ul.contributors li:after { content: ", "; }
ul.contributors li.last:after { content: ""; }

