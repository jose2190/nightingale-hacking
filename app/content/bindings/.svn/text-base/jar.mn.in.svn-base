songbird.jar:
% content  songbird  %content/songbird/
#
# Used to include operating system specific content and skin packages
% content  songbird-os-specific  %content/songbird-os-specific/ platform
#
# Include everything in this directory, and preprocess bindings.css
@include_all_to content/songbird/bindings/
* content/songbird/bindings/bindings.css (bindings.css)
* content/songbird/bindings/dialog.xml   (dialog.xml)
