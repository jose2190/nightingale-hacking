#include "nsISupports.idl"

/**
 * \interface sbIDynamicMediaList
 *
 *   This interface provides support for dynamic media lists.  These may be
 * created with sbILibrary.createMediaList("dynamic").
 */

[scriptable, uuid(683c1f61-ed50-47d3-b914-9b3ceaa999b9)]
interface sbIDynamicMediaList : nsISupports
{
  /**
   * Force an update of the dynamic media list.
   */

  void update();
};

