<deviceinfolist xmlns="http://songbirdnest.com/deviceinfo/1.0">
  <deviceinfo>
    <excludefolder match=".*"/>
    <excludefolder url="recordings"/>
    <devicecaps xmlns="http://songbirdnest.com/devicecaps/1.0">
      <audio>
        <format mime="audio/mpeg" container="audio/mpeg" codec="audio/mpeg">
          <bitrates>
            <range min="8000" max="320000" step="1" />
          </bitrates>
          <samplerates>
            <value>8000</value>
            <value>11025</value>
            <value>12000</value>
            <value>16000</value>
            <value>22050</value>
            <value>24000</value>
            <value>32000</value>
            <value>44100</value>
            <value>48000</value>
          </samplerates>
          <channels>
            <range min="1" max="2" step="1" />
          </channels>
        </format>
      </audio>
      <image>
        <format mime="image/jpeg">
          <explicit-sizes>
            <size width="200" height="200" />
          </explicit-sizes>
          <widths>
            <range min="16" max="2048" step="1" />
          </widths>
          <heights>
            <range min="16" max="2048" step="1" />
          </heights>
        </format>
      </image>
    </devicecaps>
  </deviceinfo>
</deviceinfolist>

