/* SERVICEPANE */
.servicepane-tree > treechildren::-moz-tree-image( concerts ),
.locationbar-image[properties~="concerts"],
.alltabs-item.menuitem-iconic[properties~="concerts"],
.tabbrowser-tab[properties~="concerts"] > .tab-image-middle > .tab-icon > .tab-icon-image {
	list-style-image: url(chrome://concerts/content/songkick-16x16.png);
}

