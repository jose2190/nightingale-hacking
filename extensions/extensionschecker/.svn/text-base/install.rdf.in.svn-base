<?xml version="1.0" encoding="UTF-8"?>
# vim: shiftwidth=2
#filter substitution
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
 xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>@EXTENSION_UUID@</em:id>
    <em:name>Extensions Manager Examples</em:name>
    <em:version>@EXTENSION_VER@</em:version>
    <em:creator>Aus</em:creator>
    <em:description>Extensions Manager Examples</em:description>
    <em:targetPlatform>@EXTENSION_ARCH@</em:targetPlatform>
    
    <em:targetApplication>
      <Description>
        <em:id>songbird@songbirdnest.com</em:id> <!-- songbird -->
        <em:minVersion>@EXTENSION_MIN_VER@</em:minVersion>
        <em:maxVersion>@EXTENSION_MAX_VER@</em:maxVersion>
      </Description>
    </em:targetApplication>
  </Description>
</RDF>
