<overlay id="mockcdcontroller-menu-overlay"
         xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
  
  <!-- Target the tools menu -->
  <menupopup id="menu_ToolsPopup"> 

    <!-- Add a menu item -->
    <menuitem id="mockdevicecontroller-showcontroller-menuitem"
              label="Show Mock Device Controller"
              insertafter="devToolsSeparator"
              command="mockdevicecontroller-showcontroller-cmd" />

    <menuitem id="mockcdcontroller-showcontroller-menuitem"
              label="Show Mock CD Controller"
              insertafter="devToolsSeparator"
              command="mockcdcontroller-showcontroller-cmd" />

    <menuitem id="device-caps-dump-menuitem"
              label="Dump Device Capabilities"
              insertafter="devToolsSeparator"
              command="dumpdevicecaps-cmd" />
    <menuitem id="fakepds-menuitem"
              label="Fake PDS"
              insertafter="devToolsSeparator"
              command="fakepds-cmd" />
  </menupopup>

</overlay>
