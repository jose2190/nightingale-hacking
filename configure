            { $as_echo "$as_me:${as_lineno-$LINENO}: result: $DBUS_LIBS" >&5
$as_echo "$DBUS_LIBS" >&6; }
        else
            DBUS_CFLAGS=""
            DBUS_LIBS=""
            ## If we have a custom action on failure, don't print errors, but
