  withval=$with_media_core;  SB_MEDIA_CORES=$withval
else
   SB_MEDIA_CORES=default
fi

for option in `echo $SB_MEDIA_CORES | sed 's/,/ /g'`; do
    if test "$option" = "default"; then
         case "$SB_PLATFORM" in
            windows)
                MEDIA_CORE_GST=1
                MEDIA_CORE_WMP=1
                MEDIA_CORE_QT=1
            ;;
            macosx)
                # Enable GST only on x86 (we don't have PPC binaries)
                case "$SB_ARCH" in
                    *86)
                        MEDIA_CORE_GST=1
                    ;;
                esac
                MEDIA_CORE_QT=1
            ;;
            linux|solaris)
                MEDIA_CORE_GST=1
            ;;
            *)
                as_fn_error $? "No platform default media cores found; please manually specify --with-media-core=" "$LINENO" 5
            ;;
        esac
    elif test "$option" = "none"; then
        # Nothing needs to be set here
        true
    elif test "$option" = "windowsmedia"; then
        MEDIA_CORE_WMP=1
    elif test "$option" = "gstreamer"; then
        MEDIA_CORE_GST=1
    elif test "$option" = "gstreamer-system"; then
        MEDIA_CORE_GST_SYSTEM=1
    elif test "$option" = "qt"; then
        MEDIA_CORE_QT=1
    else
        as_fn_error $? "Unknown --with-media-core option $option" "$LINENO" 5
    fi

    if test "$MEDIA_CORE_GST" = "1" -a "$MEDIA_CORE_GST_SYSTEM" = "1"; then
        as_fn_error $? "You can not build the gstreamer and gstreamer-system media cores at the same time" "$LINENO" 5
    fi
done





