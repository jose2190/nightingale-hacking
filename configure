#
# Check platform
#

case "$target" in
  *-cygwin*|*-mingw*|*-msvc*|*-mks*)
    SB_PLATFORM="windows"
    case "$target_cpu" in
      *i*86*)
        SB_ARCH="$target_cpu"
      ;;
      *)
        as_fn_error $? "Unsupported architecture" "$LINENO" 5
      ;;
    esac
    # Check that we're MSYS
    case "$target" in
      *-mingw*)
        # no cygwin wrapper for mingw
      ;;
      *)
        as_fn_error $? "Songbird must be built using MozillaBuild/MSYS" "$LINENO" 5
      ;;
    esac
  ;;
  *darwin*)
    SB_PLATFORM="macosx"
    case "$target_cpu" in
      powerpc* | ppc)
        SB_ARCH="ppc"
      ;;
      *i*86*)
        SB_ARCH="$target_cpu"
      ;;
      *)
        as_fn_error $? "Unsupported architecture" "$LINENO" 5
      ;;
    esac
  ;;
  *linux*)
    SB_PLATFORM="linux"
    case "$target_cpu" in
      *86)
        SB_ARCH="$target_cpu"
      ;;
      x86_64)
        SB_ARCH="$target_cpu"
      ;;
      powerpc | ppc)
        SB_ARCH="ppc"
      ;;
      powerpc64 | ppc64)
        SB_ARCH="ppc64"
      ;;
      *)
        as_fn_error $? "Unsupported architecture" "$LINENO" 5
      ;;
    esac
  ;;
  *solaris*)
    SB_PLATFORM="solaris"
    case "$target_cpu" in
      *86)
        SB_ARCH="$target_cpu"
      ;;
      x86_64)
        SB_ARCH="$target_cpu"
      ;;
      sparc)
        SB_ARCH="$target_cpu"
      ;;
      *)
        as_fn_error $? "Unsupported architecture" "$LINENO" 5
      ;;
    esac
  ;;
  *)
    as_fn_error $? "Unsupported platform $target" "$LINENO" 5
  ;;
esac



#
#  Check for --enable-installer
#

# Always set SB_INSTALLER_SUFFIXES so that the installer can be built
# manually as expected. These are the defaults.

case $SB_PLATFORM in
  windows) SB_INSTALLER_SUFFIXES=exe;;
  macosx)  SB_INSTALLER_SUFFIXES=dmg;;
  *)       SB_INSTALLER_SUFFIXES=tar.gz;;
esac

# Now process --enable-installer options.

# Check whether --enable-installer was given.
if test "${enable_installer+set}" = set; then :
  enableval=$enable_installer; enable_installer=`echo $enableval | tr 'A-Z,' 'a-z '`
fi

case $enable_installer in
  yes)
    MAKE_INSTALLER=1
    ;;
  no)
    { MAKE_INSTALLER=; unset MAKE_INSTALLER;}
    ;;
  *)
    { MAKE_INSTALLER=; unset MAKE_INSTALLER;}
    for word in $enable_installer; do
      case $word in
        nsis)
          if test $SB_PLATFORM = "windows"; then :
  SB_INSTALLER_SUFFIXES="$SB_INSTALLER_SUFFIXES $word"
else
  as_fn_error $? "NSIS installer not supported on $SB_PLATFORM" "$LINENO" 5
fi
          MAKE_INSTALLER=1
          ;;
        dmg|pkg)
          if test $SB_PLATFORM = "macosx"; then :
  SB_INSTALLER_SUFFIXES="$SB_INSTALLER_SUFFIXES $word"
else
  as_fn_error $? "$word installer not supported on $SB_PLATFORM" "$LINENO" 5
fi
          MAKE_INSTALLER=1
          ;;
        tar.gz)
          if test $SB_PLATFORM != "windows" -a $SB_PLATFORM != "macosx"; then :
  SB_INSTALLER_SUFFIXES="$SB_INSTALLER_SUFFIXES $word"
else
  as_fn_error $? "$word installer not supported on $SB_PLATFORM" "$LINENO" 5
fi
          MAKE_INSTALLER=1
          ;;
        *)
          as_fn_error $? "Installer type $word not recognized" "$LINENO" 5
          ;;
      esac
    done
    ;;
esac



