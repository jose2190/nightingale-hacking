    if test -z "$RANLIB"; then as_fn_error $? "Ranlib not found" "$LINENO" 5; fi
    if test -z "$GZIP"; then as_fn_error $? "Gzip not found" "$LINENO" 5; fi
    if test -z "$GUNZIP"; then as_fn_error $? "Gunzip not found" "$LINENO" 5; fi
